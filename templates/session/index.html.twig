<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootswatch@4.5.2/dist/cerulean/bootstrap.min.css" integrity="sha384-3fdgwJw17Bi87e1QQ4fsLn4rUFqWw//KU0g8TvV6quvahISRewev6/EocKNuJmEw" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
<div class="container">
    {% for value in app.session.flashBag.get("welcome") %}
    <div class="alert alert-info">{{ value }}</div>
    {% endfor %}
    {% for value in app.session.flashBag.get("success") %}
        <div class="alert alert-success"> {{ value }}</div>
    {% endfor %}
    {% for value in app.session.flashBag.get("error") %}
        <div class="alert alert-danger">{{ value }}</div>
    {% endfor %}
    <ul class="list-group">
        <li class="list-group-item active" aria-current="true">La Liste Des TODOS :</li>
        {% for cle , value in app.session.get("todos")%}
            <li class="list-group-item">{{cle}} : {{value}}</li>
        {% endfor %}
    </ul>

</div>
</body>
</html>